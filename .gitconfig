[color]
  ui = true
[user]
  name = Kenneth Lu
  email = lu.yang.kenneth@gmail.com
[core]
  excludesfile = ~/.gitignore_global
  editor = sublime -w
[alias]
  fm = fetch origin master:master
  fp = fetch --prune
  st = status
  br = branch
  co = checkout
  cob = checkout -b
  ci = commit
  cm = commit -m
  ca = commit --amend
  pushf = push --force-with-lease
  di = diff
  dh = diff HEAD~1
  ds = diff --staged
  discard = checkout -- .
  unstage = reset HEAD --
  lg = log -8 --graph --pretty=format:'%C(red)%h%C(reset)%C(yellow)%d%C(reset) %s'
  la = log --graph --pretty=format:'%C(red)%h%C(reset)%C(yellow)%d%C(reset) %s'
  la2 = log --graph --pretty=format:'%C(red)%h%C(reset)%C(yellow)%d%C(reset) %s %C(cyan)(%cr by %cN)%C(reset)'
[push]
  default = current
